{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI Uvicorn",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.app:app",
        "--host", "127.0.0.1",
        "--port", "5500",
        "--reload"
      ],
      "jinja": true,
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
  